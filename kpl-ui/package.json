{
  "name": "kpl-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test:e2e": "playwright test",
    "db:init": "ts-node -r tsconfig-paths/register scripts/db.ts init",
    "db:seed": "ts-node -r tsconfig-paths/register scripts/db.ts seed",
    "db:reset": "ts-node -r tsconfig-paths/register scripts/db.ts reset",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "migrate:undo:all": "npx sequelize-cli db:migrate:undo:all",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo",
    "seed:undo:all": "npx sequelize-cli db:seed:undo:all"
  },
  "dependencies": {
    "@ant-design/icons": "^5.3.7",
    "@aws-sdk/client-s3": "^3.927.0",
    "@aws-sdk/s3-request-presigner": "^3.927.0",
    "@imagekit/next": "^2.1.3",
    "@types/three": "^0.181.0",
    "antd": "^5.12.5",
    "axios": "^1.6.2",
    "bcryptjs": "^3.0.3",
    "constate": "^3.3.2",
    "imagekit": "^6.0.0",
    "mongodb": "^6.3.0",
    "multer": "^2.0.2",
    "next": "14.0.4",
    "next-auth": "^4.24.13",
    "pg": "^8.16.3",
    "pg-hstore": "^2.3.4",
    "react": "^18",
    "react-confetti": "^6.1.0",
    "react-confetti-explosion": "^2.1.2",
    "react-dom": "^18",
    "sass": "^1.69.5",
    "sequelize": "^6.37.7",
    "sharp": "^0.34.5",
    "three": "^0.181.2",
    "uuid": "^13.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@types/bcryptjs": "^2.4.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/uuid": "^10.0.0",
    "@types/validator": "^13.15.4",
    "autoprefixer": "^10.0.1",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "postcss": "^8",
    "sequelize-cli": "^6.6.3",
    "tailwindcss": "^3.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5"
  }
